{
    "name": "ToDoList",
    "description": "Manage your time, schedule your tasks",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "scripts/background.js"
      },
    "permissions": [
        "storage", "activeTab", "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/toDo16.png",
            "32": "/images/toDo32.png",
            "48": "/images/toDo48.png",
            "128": "/images/toDo128.png"
        }
    },
    "icons": {
        "16": "/images/toDo16.png",
        "32": "/images/toDo32.png",
        "48": "/images/toDo48.png",
        "128": "/images/toDo128.png"
    }
   
}